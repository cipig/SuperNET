#!/bin/bash

cd secp256k1; ./m_unix; cd ..
cd ../crypto777; ./m_LP_fast; cd ../iguana
clang -g0 -Wno-deprecated -c -march=native -O2 -DISNOTARYNODE=1 -DLIQUIDITY_PROVIDER=1 *.c ../basilisk/basilisk.c ../gecko/gecko.c ../datachain/datachain.c
clang -g0 -Wno-deprecated -c -march=native -O2 -DISNOTARYNODE=1 -DLIQUIDITY_PROVIDER=1  main.c iguana777.c iguana_bundles.c ../basilisk/basilisk.c
clang -g0 -march=native -o ../agents/iguana-fast *.o ../agents/libcrypto777.a -lnanomsg -lcurl -lssl -lcrypto -lpthread -lz -lm -lsodium
